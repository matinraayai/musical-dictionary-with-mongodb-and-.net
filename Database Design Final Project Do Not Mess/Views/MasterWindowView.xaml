<Window x:Class="Database_Design_Final_Project_Do_Not_Mess.Views.MasterWindowView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Database_Design_Final_Project_Do_Not_Mess.Views"
        mc:Ignorable="d"
        Title="MasterWindowView" Height="300" Width="300">
    <Grid>
        <ScrollViewer HorizontalAlignment="Left" Height="595" VerticalAlignment="Top" Width="492" Margin="0,0,0,-126">
            <Grid>
                <TextBlock x:Name="name" HorizontalAlignment="Left" Height="25" VerticalAlignment="Top" Width="440" Margin="20,20,0,-126" TextWrapping="Wrap" FontWeight="Bold" FontSize="18"/>
                <TextBlock x:Name="profile" HorizontalAlignment="Left" Height="500" VerticalAlignment="Top" Width="440" Margin="20,60,0,-126" TextWrapping="Wrap" FontWeight="Bold" FontSize="18"/>
            </Grid>
        </ScrollViewer>
    </Grid>
</Window>
